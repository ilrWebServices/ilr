{#
/**
 * @file
 * Theme override for a field.
 *
 * @see template_preprocess_field()
 */
#}
{% set content %}
  {% for item in items %}
    {{ item.content }}
  {% endfor %}
{% endset %}

{% include '@union/_card.twig' with {
  card_content: content,
  attributes: attributes.addClass([
    'cu-card--padded cu-card--vibrant',
  ])
} only %}
